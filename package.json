    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rxjs": "^5.4.2",
    "stylelint": "^7.13.0",
    "tslint": "^5.5.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.4.1"
