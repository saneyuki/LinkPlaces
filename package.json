  "bugs": {
    "url": "https://github.com/saneyuki/linkplaces/issues"
  },
  "homepage": "https://github.com/saneyuki/linkplaces",
  "dependencies": {
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
