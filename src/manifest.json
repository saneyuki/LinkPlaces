{
  "manifest_version": 2,
  "name": "LinkPlaces",
  "version": "0.0.1",
  "description": "This provides Sidebar to display \"Unsorted Bookmarks\" folder in Places, as \"Temporary Links\"",
  "applications": {
    "gecko": {
      "id": "linkplaces.webext@saneyuki_s",
      "strict_min_version": "55.0"
    }
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "page": "background/index.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "toolbaricon.svg",
    "default_title": "LinkPlaces",
    "default_popup": "popup/index.html"
  },
  "sidebar_action": {
    "browser_style": true,
    "default_icon": "toolbaricon.svg",
    "default_title": "LinkPlaces WebExt",
    "default_panel": "sidebar/index.html"
  },
  "options_ui": {
    "browser_style": true,
    "page": "options/index.html"
  }
}
