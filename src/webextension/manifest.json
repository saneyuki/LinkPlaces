{
  "manifest_version": 2,
  "name": "LinkPlaces",
  "version": "0.0.1",
  "permissions": [
    "bookmarks",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "bundled_background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "toolbaricon.svg",
    "default_title": "LinkPlaces popup (WebExt)",
    "default_popup": "popup/index.html"
  },
  "sidebar_action": {
    "default_icon": "toolbaricon.svg",
    "default_title": "LinkPlaces WebExt",
    "default_panel": "sidebar/index.html"
  }
}
